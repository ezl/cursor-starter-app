    "url": "https://github.com/ezl/cursor-starter-app/issues"
  },
  "homepage": "https://github.com/ezl/cursor-starter-app#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10"
  }
